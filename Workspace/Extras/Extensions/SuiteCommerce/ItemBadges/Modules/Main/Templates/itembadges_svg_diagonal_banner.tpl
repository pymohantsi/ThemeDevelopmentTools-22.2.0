<div class="itembadges-badge-container-individual itembadges-badge-container-individual-large">
  <svg class="itembadges-badge-svg-{{position}}" width="103px" height="102px" viewBox="0 0 103 102" role="img" aria-label="{{alt}}" version="1.1" xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
    <title>{{alt}}</title>
    <g id="Badges" stroke="none" stroke-width="1" fill="none" fill-rule="evenodd">
      <g id="Badges">
        <g id="Diagonal-Banner">
          <rect id="Diagonal Banner" fill="{{textBgColor}}" x="-10" y="0" width="140" height="{{svg.height}}" transform="rotate({{#if flipVertical}} 45, 45, 45 {{else}} -45, 65, 65 {{/if}})"></rect>
          <text id="{{text}}" class="item-badges-svg-text" transform="rotate({{#if flipVertical}} 45, 45, 45 {{else}} -45, 65, 65 {{/if}})" font-weight="{{textWeight}}" line-spacing="1" fill="{{textColor}}">
            <tspan text-anchor="middle" x="66.5" y="{{svg.textCenter}}">{{text}}</tspan>
          </text>
        </g>
      </g>
    </g>
  </svg>
</div>
